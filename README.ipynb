    "{-# LANGUAGE OverloadedStrings #-}\n",
    "\n",
    "import Highcharts\n",
    "import Data.Aeson\n",
    "\n",
    "Chart $ object\n",
    "  [ \"title\" .=\n",
    "      object [ \"text\" .= \"some title!!!\" ]\n",
    "  , \"series\" .=\n",
    "      [ object [\"name\" .= \"bar\", \"data\" .= [1,2,3,4]] ]\n",
    "  ]"
